{
  "name": "tsaws",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@swc/core": "^1.3.37",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@tsaws/eslint-config-custom": "workspace:*",
    "@types/node": "^18.14.6",
    "prettier": "^2.8.7",
    "rollup": "^2.79.1",
    "rollup-plugin-delete": "^1.2.0",
    "rollup-plugin-node-externals": "^5.1.2",
    "rollup-plugin-swc3": "^0.8.0",
    "rollup-plugin-typescript2": "^0.32.1",
    "tslib": "^2.5.0",
    "typescript": "~4.9.5",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "^node:",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  },
  "packageManager": "pnpm@7.28.0"
}
