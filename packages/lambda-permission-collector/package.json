{
  "name": "@tsaws/lambda-permission-collector",
  "version": "0.0.1",
  "description": "Collects required IAM permissions by analyzing code.",
  "license": "MIT",
  "author": "William Feng <william.feng91@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/williamfeng91/tsaws.git",
    "directory": "packages/lambda-permission-collector"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin rollup-plugin-swc3",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "typecheck": "vitest typecheck --no-watch",
    "typecheck:watch": "vitest typecheck"
  },
  "dependencies": {
    "@babel/parser": "^7.21.2",
    "@babel/traverse": "^7.21.2",
    "@babel/types": "^7.21.2",
    "@swc/core": "^1.3.40"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.111",
    "@types/babel__traverse": "^7.18.3",
    "@types/node": "^18.14.6",
    "@vitest/coverage-istanbul": "^0.29.2",
    "aws-sdk": "^2.1334.0",
    "rollup": "^2.79.1",
    "tslib": "^2.5.0",
    "typescript": "~4.9.5",
    "vitest": "^0.29.2"
  }
}
